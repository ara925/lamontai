{
  "name": "lamontai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:css": "tailwindcss -i ./src/app/globals.css -o ./dist.css",
    "copy:css": "node -e \"require('fs').copyFileSync('./dist.css', './public/dist.css')\"",
    "dev:css": "npm run build:css && npm run copy:css && next dev",
    "rebuild": "rm -rf node_modules/.cache && rm -rf .next && npm install && npx tailwindcss init -p && npm run build:css && npm run dev",
    "prepare:prod": "npm run build:css && npm run copy:css && next build",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "test:auth": "node --experimental-modules run-test.mjs",
    "test:e2e": "node --experimental-modules e2e-auth-test.mjs"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "20.11.30",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "10.4.18",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.0",
    "cookies-next": "^5.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.14",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.487.0",
    "next": "14.1.3",
    "next-auth": "^4.24.11",
    "node-fetch": "^2.7.0",
    "pg": "^8.14.1",
    "postcss": "8.4.35",
    "react": "18.2.0",
    "react-cookie": "^8.0.1",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "cssnano": "^7.0.6",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prisma": "^6.6.0",
    "puppeteer": "^24.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "ts-node": "^10.9.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
