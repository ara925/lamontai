{
  "name": "lamontai",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <23.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "build:windows": "npx prisma generate && node src/lib/fix-prisma.js && next build",
    "build:docker": "prisma generate && next build",
    "fix:windows": "node src/lib/fix-prisma.js",
    "start": "next start -p 3001",
    "start-local": "next dev -p 3001 --turbo",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "prisma:push": "prisma db push",
    "build:full": "next build",
    "build:css": "tailwindcss -i ./src/app/globals.css -o ./dist.css",
    "copy:css": "node -e \"require('fs').copyFileSync('./dist.css', './public/dist.css')\"",
    "dev:css": "npm run build:css && npm run copy:css && next dev",
    "rebuild": "rm -rf node_modules/.cache && rm -rf .next && npm install && npx tailwindcss init -p && npm run build:css && npm run dev",
    "prepare:prod": "npm run build:css && npm run copy:css && npm run build:full",
    "cloudflare:prepare": "node prepare-for-cloudflare.js",
    "cloudflare:build": "node prepare-for-cloudflare.js && npx cross-env NODE_ENV=production NEXT_PUBLIC_DEPLOY_ENV=cloudflare next build",
    "cloudflare:init-db": "node scripts/init-cloudflare-db.js",
    "cloudflare:dev": "npx cross-env NEXT_PUBLIC_DEPLOY_ENV=cloudflare NEXT_PUBLIC_CLOUDFLARE_ENABLED=true next dev -p 3001",
    "cloudflare:test-edge": "npx cross-env NEXT_PUBLIC_DEPLOY_ENV=cloudflare NEXT_PUBLIC_CLOUDFLARE_ENABLED=true next build && next start -p 3001",
    "cloudflare:deploy": "node prepare-for-cloudflare.js && npx cross-env NODE_ENV=production NEXT_PUBLIC_DEPLOY_ENV=cloudflare next build && npx @cloudflare/next-on-pages",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "test:auth": "node --experimental-modules run-test.mjs",
    "test:e2e": "node --experimental-modules e2e-auth-test.mjs",
    "test:auth-debug": "node scripts/auth-test.js",
    "db:backup": "node src/lib/backup-scripts/db-backup.js",
    "db:restore": "node src/lib/backup-scripts/db-restore.js",
    "db:vacuum": "node src/lib/backup-scripts/database-vacuum.js",
    "monitor": "node src/lib/backup-scripts/system-monitor.js",
    "logs:rotate": "node src/lib/backup-scripts/logger-rotation.js",
    "setup:cron": "node src/lib/backup-scripts/setup-cron.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.1.0",
    "@headlessui/react": "^1.7.18",
    "@neondatabase/serverless": "^0.7.1",
    "@prisma/adapter-neon": "^6.6.0",
    "@prisma/client": "^6.6.0",
    "@vercel/kv": "^3.0.0",
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0",
    "cloudinary": "^1.41.3",
    "clsx": "^2.1.1",
    "fast-xml-parser": "^5.2.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.7.2",
    "glob": "^10.3.10",
    "ioredis": "^5.6.1",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.488.0",
    "next": "14.1.3",
    "next-auth": "^4.24.5",
    "openai": "^4.94.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "rimraf": "^5.0.5",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "txml": "^5.1.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "18.3.20",
    "@types/react-dom": "^18",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "assert": "^2.1.0",
    "autoprefixer": "^10.0.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "bufferutil": "^4.0.9",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.3",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prisma": "^6.6.0",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "util": "^0.12.5"
  },
  "overrides": {
    "are-we-there-yet": "^4.0.1",
    "gauge": "^6.0.0",
    "npmlog": "^8.0.0"
  },
  "resolutions": {
    "are-we-there-yet": "^4.0.1",
    "gauge": "^6.0.0",
    "npmlog": "^8.0.0"
  }
}
